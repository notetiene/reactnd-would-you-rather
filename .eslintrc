{
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "globals": {
    "ENV": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 10,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    },
    "sourceType": "module"
  },
  "extends": "airbnb",
  "rules": {
    "react/jsx-fragments": 0
  },
  "plugins": ["jest"]
}
